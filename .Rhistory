totalpass <- function(data) {
ifelse(data[2] == "Throwaway" || data[2] == "Drop" || data[2] == "Stall" || data[2] == "Callahan", data[3] <- as.numeric(data[3]) * -1,
ifelse(data[2] == "Goal", as.numeric(data[3]), 0)
)
}
